<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.examplemonevo.Controllers.TransactionHistoryController">
   <AnchorPane prefHeight="553.0" prefWidth="760.0" style="-fx-background-color: #FFF9EE;" stylesheets="@style.css">
      <children>
      
                <Button fx:id="backButton" layoutX="322.0" layoutY="507.0" styleClass="login-button" text="Back to Dashboard" />
         <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="280.0" layoutY="-20.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../logo.png" />
            </image>
         </ImageView>
          <TableView fx:id="transactionTable" layoutX="80.0" layoutY="91.0" prefHeight="400" prefWidth="600">
              <columns>
                  <TableColumn fx:id="transactionIdColumn" prefWidth="114.0" text="Transaction ID" />
                  <TableColumn fx:id="accountNumberColumn" prefWidth="120.0" text="Account Number" />
                  <TableColumn fx:id="amountColumn" minWidth="6.0" prefWidth="64.0" text="Amount" />
                  <TableColumn fx:id="typeColumn" minWidth="4.0" prefWidth="50.0" text="Type" />
                  <TableColumn fx:id="descriptionColumn" prefWidth="122.0" text="Description" />
                  <TableColumn fx:id="dateColumn" prefWidth="129.0" text="Date" />
              </columns>
          </TableView>
      </children>
   </AnchorPane>
</VBox>
